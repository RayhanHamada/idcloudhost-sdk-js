{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "xo",
  "overrides": [
    {
      "extends": ["xo-typescript"],
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "object-curly-spacing": "off",
        "@typescript-eslint/object-curly-spacing": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {}
}
